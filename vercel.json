{
  "version": 2,
  "builds": [
    {
      "src": "src/project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": "src/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/project/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "src"
  }
}